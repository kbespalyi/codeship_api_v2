{
  "name": "codeship_api_v2",
  "version": "1.0.0",
  "description": "Demo for codeship api v2",
  "author": "Khusein Bespalyi",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "docker build -t kbespalyi/codeship_api_v2 .",
    "start": "docker run --rm -it -p 3000:3000 -v $(pwd)/controllers:/app/controllers -v $(pwd)/views:/app/views -v $(pwd)/clients:/app/clients -v $(pwd)/data-access:/app/data-access --env-file .env kbespalyi/codeship_api_v2",
    "daemon": "docker run -d --restart always -p 80:3000 --env-file .env kbespalyi/codeship_api_v2 node index.js",
    "test": "DB=mongodb NODE_ENV=test ./node_modules/.bin/nyc mocha -R spec test/tests/**/*.js test/tests/**/**/*.js test/tests/**/**/**/*.js "
  },
  "dependencies": {
    "express": "^4.15.4",
    "express-handlebars": "^3.0.0",
    "node-cache": "^4.1.1",
    "nodemon": "^1.11.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "bluebird": "~3.5.1",
    "bson-objectid": "1.2.0",
    "busboy": "0.2.14",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "cron": "~1.3.0",
    "csv": "~1.1.1",
    "lodash": "~4.17.4",
    "mocha-lcov-reporter": "^1.3.0",
    "moment": "~2.19.1",
    "moment-timezone": "^0.5.13",
    "mongodb": "2.2.33",
    "node-fetch": "^1.7.3",
    "xml2js": "^0.4.19",
    "xml2json": "~0.11.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-things": "0.2.0",
    "mocha": "3.5.3",
    "mocha-circleci-reporter": "0.0.2",
    "mocha-unfunk-reporter": "0.4.0",
    "nyc": "^11.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kbespalyi/codeship_api_v2.git"
  },
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "9.0.0",
    "npm": "5.5.1"
  },
  "bugs": {
    "url": "https://github.com/kbespalyi/codeship_api_v2/issues"
  },
  "homepage": "https://github.com/kbespalyi/codeship_api_v2#readme"
}
